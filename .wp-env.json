{
	"$schema": "https://schemas.wp.org/trunk/wp-env.json",
	"core": null,
	"plugins": [
		"https://downloads.wordpress.org/plugin/stream.zip",
		"./."
	],
	"env": {
		"development": {
			"config": {
				"WP_DEVELOPMENT_MODE": "plugin",
				"WP_DEBUG_LOG": "/var/www/html/wp-content/plugins/onemedia/tests/_output/debug.log"
			}
		},
		"tests": {
			"config": {
				"FS_METHOD": "direct"
			},
			"plugins": [
				"."
			],
			"mappings": {
				"/wp-content/plugins/onemedia": "."
			}
		}
	},
	"lifecycleScripts": {
		"afterStart": "wp-env run cli wp rewrite structure /%postname%/ --hard"
	}
}
